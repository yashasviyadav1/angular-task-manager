{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/frontend"
        }
      }
    ],
    "routes": [
      {
        "src": "/(assets/.+)",
        "headers": { "cache-control": "public, max-age=31536000, immutable" }
      },
      {
        "src": "/(.+\\.(?:css|js))",
        "headers": { "cache-control": "public, max-age=31536000, immutable" }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }